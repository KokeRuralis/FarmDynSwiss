<?xml version="1.0" encoding="UTF-8"?>
<GGIG>
         <activitydim><attr>2</attr></activitydim>
         <agpspread.viewsize><attr>1913,1097</attr></agpspread.viewsize>
         <AgpMapPanel.debugMaps><attr>false</attr></AgpMapPanel.debugMaps>
         <allTasks><attr>Experiments dairy,Experiments beef,Experiments arable,Experiments fattners,Experiments sows, Experiments biogas, Calculate MACs,Single farm run,Farm sample run</attr></allTasks>
         <nonSampleTasks><attr>Experiments dairy,Experiments beef,Experiments arable,Experiments fattners,Experiments sows, Experiments biogas, Calculate MACs,Single farm run</attr></nonSampleTasks>
         <expTasks><attr>Experiments dairy,Experiments arable,Experiments fattners,Experiments sows, Experiments biogas</attr></expTasks>
         <gamsOptionsSys><attr>optdir=opt</attr></gamsOptionsSys>
         <defaultDirs><attr>model,res,dat</attr></defaultDirs>
         <datdir><attr>../dat</attr></datdir>
         <modeldir><attr>../gams</attr></modeldir>
         <resdir><attr>../results</attr></resdir>

         <!-- <inactiveSettingsToIncludeFile><attr>true</attr></inactiveSettingsToIncludeFile> -->

         <helpmenuitem>
                  <name>View FARMDYN web page</name>
                  <value>http://www.ilr.uni-bonn.de/agpo/rsrch/farmdyn/farmdyn_e.htm</value>
         </helpmenuitem>

         <helpmenuitem>
                  <name>Open FARMDYN model documentation</name>
                  <value>../doc/farmdyn_docu.pdf</value>
                  <type>pdf</type>
         </helpmenuitem>

          <allowAdmin><attr>yes</attr></allowAdmin>
          <spread><attr>true</attr></spread>

         <control>
                  <Type>tab</Type>
                  <Title>General settings</Title>
                  <tasks>%allTasks%</tasks>
         </control>
          <control>
                  <Type>separator</Type>
                  <title>Crop Data Source</title>
                  <tasks>Build data from user input</tasks>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Crop Database Constructor</Title>
                  <Value>cropsData_default.gms</Value>
                  <options>%datdir%/User/cropsData_*.gms</options>
                  <gamsName>CropDataSource</gamsName>
                  <tasks>Build data from user input</tasks>
         </control>
         <control>
                  <Type>separator</Type>
                  <title>Conventional farming</title>
                  <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Cereals_conv</Title>
                 <Value>true</Value>
                 <style>title:Cereals</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <Title>Crop selection</Title>
                 <range>-20</range>
                 <Title>CerealsConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=CerealsConv</options>
                 <style>title:none,output:set</style>
                 <dependsOn>Cereals_conv:true</dependsOn>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Fodder crops_conv</Title>
                 <Value>True</Value>
                 <style>title:Fodder crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>FodderConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=FodderConv</options>
                 <style>title:none,output:set</style>
                 <dependsOn>Fodder crops_conv:true</dependsOn>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Energy crops_conv</Title>
                 <Value>true</Value>
                 <style>title:Energy crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>EnergyConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=EnergyConv</options>
                 <style>title:none,output:set</style>
                 <dependsOn>Energy crops_conv:true</dependsOn>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Root crops_conv</Title>
                 <Value>True</Value>
                 <style>title:Root crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>RootCropsConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=RootCropsConv</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Root crops_conv:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Oil and protein crops_conv</Title>
                 <Value>TRUE</Value>
                 <style>title:Oil and protein crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>OilAndProteinConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=OilAndProteinConv</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Oil and protein crops_conv:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Catch crops_conv</Title>
                 <Value>true</Value>
                 <style>title:Catch crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>CatchCropsConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=CatchCropsConv</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Catch crops_conv:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Vegetables_conv</Title>
                 <Value>true</Value>
                 <style>title:Vegetables</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>VegetablesConv</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=VegetablesConv</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Vegetables_conv:true</dependsOn>
         </control>
         <control>
                  <Type>separator</Type>
                  <title>Organic farming</title>
                  <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Cereals_Organic</Title>
                 <Value>true</Value>
                 <style>title:Cereals</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>CerealsOrg</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=CerealsOrg</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Cereals_Organic:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Fodder Organic</Title>
                 <Value>true</Value>
                 <style>title:Fodder crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>FodderOrg</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=FodderOrg</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Fodder Organic:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Root crops organic</Title>
                 <Value>true</Value>
                 <style>title:Root crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>RootCropsOrg</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=RootCropsOrg</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Root crops organic:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Oil and protein crops organic</Title>
                 <Value>true</Value>
                 <style>title:Oil and protein crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>OilAndProteinOrg</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=OilAndProteinOrg</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Oil and protein crops organic:true</dependsOn>
         </control>

         <control>
                 <Type>checkBox</Type>
                 <Title>Catch crops organic</Title>
                 <Value>true</Value>
                 <style>title:Catch crops</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>CatchCropsOrg</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=CatchCropsOrg</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Catch crops organic:true</dependsOn>
         </control>
         <control>
                 <Type>checkBox</Type>
                 <Title>Vegetables Organic</Title>
                 <Value>true</Value>
                 <style>title:Vegetables</style>
                 <tasks>Build data from KTBL</tasks>
         </control>
         <control>
                 <Type>MultiListNotZero</Type>
                 <range>-20</range>
                 <Title>VegetablesOrg</Title>
                 <options>%modeldir%/ktbl/ktbl.gdx,setname=VegetablesOrg</options>
                 <style>title:none,output:set</style>
                 <tasks>Build data from KTBL</tasks>
                 <dependsOn>Vegetables Organic:true</dependsOn>
         </control>
         <control>
                  <type>dirsel</type>
                  <Title>batchOutputDir</Title>
                  <options>%scratchdir%\*.*</options>
                  <gamsName>batchOutputDir</gamsName>
                  <tasks>Combine summary results</tasks>
         </control>

         <control>
                  <Type>separator</Type>
                  <Title>Branches</Title>
                  <tasks>%allTasks%</tasks>
         </control>

         <control>
                  <Type>text</Type>
                  <Title>Scenario description</Title>
                  <Value>enterName</Value>
                  <gamsName>scenDes</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>width:250,title:Scenario\ndescription,spread:true</style>
         </control>

         <control>
                  <Type>singlelist</Type>
                  <Title>Type of experiment</Title>
                  <Value>MAC</Value>
                  <options>MAC,Profits,Fertilizer Directive,Multi indicator</options>
                  <gamsName>scenType</gamsName>
                  <tasks>%expTasks%</tasks>
         </control>

         <control>
                  <Type>fileSel</Type>
                  <Title>Farm sample file</Title>
                  <Value>farmData_de</Value>
                  <options>%datdir%/farmData_*.gdx</options>
                  <gamsName>farmSampleFile</gamsName>
                  <tasks>Farm sample run</tasks>
         </control>

         <control>
                  <Type>MultiListNotZero</Type>
                  <Title>farmIds</Title>
                  <range>-2</range>
                  <options>%datdir%/%%task%.farm Sample File%.gdx,setname=farmIds</options>
                  <gamsName>farmIds</gamsName>
                  <style>titlePos:onTop,width:100</style>
                  <tasks>Farm sample run</tasks>
                  <dependsOn>farm Sample File</dependsOn>
         </control>
         <control>
                  <Type>MultiListNotZero</Type>
                  <Title>Farm branches</Title>
                  <range>-2</range>
                  <Value>Dairy,Arable</Value>
                  <options>Dairy,MotherCows,Beef,Sows,Fattners,Arable,Biogas</options>
                  <gamsName>farmBranch</gamsName>
                  <style>titlePos:onTop</style>
                  <tasks>%nonSampleTasks%</tasks>
        </control>
         <control>
                  <Type>MultiListNotZero</Type>
                  <Title>Farm branches</Title>
                  <range>-2</range>
                  <Value>all</Value>
                  <options>Dairy,MotherCows,Beef,Sows,Fattners,Arable,Biogas</options>
                  <gamsName>farmBranch</gamsName>
                  <style>titlePos:onTop</style>
                  <userLevls>debugger:disable,developper:disable,Runner:disable,Administrator:disable</userLevls>
                  <tasks>Farm sample run</tasks>
        </control>


         <control>
                  <Type>Singlelist</Type>
                  <Title>Definition of herd size</Title>
                  <Value>Number</Value>
                  <Options>Number,LU per ha</Options>
                  <gamsName>DefineHerdSize</gamsName>
                  <dependsOn>Farm branches:Dairy,Fattners,Beef,MotherCows,Sows</dependsOn>
                  <style>noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>

         <control>
                  <Type>separator</Type>
                  <Title>Data</Title>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>cropsFile</Title>
                  <Value>crops_ch</Value>
                  <options>%datdir%/crops_*.gdx</options>
                  <gamsName>cropsFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>title:Crop data,output:none,vAlignment:Spread,spread:true</style>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>cropsGmsFile</Title>
                  <Value>crops_ch</Value>
                  <options>%datdir%/crops_*.gms</options>
                  <gamsName>cropsGmsFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>title:Crops file,output:none,vAlignment:Spread</style>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>CropOpFile</Title>
                  <Value>cropop_de</Value>
                  <options>%datdir%/cropop_*.gms</options>
                  <gamsName>cropOpFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>title:Crop Operations,output:none,vAlignment:Spread</style>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Machinery</Title>
                  <Value>mach_de</Value>
                  <options>%datdir%/mach_*.gms</options>
                  <gamsName>machFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>output:none,vAlignment:Spread</style>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Feeds</Title>
                  <Value>feeds_de</Value>
                  <options>%datdir%/feeds_*.gms</options>
                  <gamsName>feedsFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>output:none,vAlignment:Spread</style>
                  <dependsOn>Farm branches:Dairy,Fattners,Beef,MotherCows,Sows</dependsOn>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Stables</Title>
                  <Value>stables_de</Value>
                  <options>%datdir%/stables_*.gms</options>
                  <gamsName>stableFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>output:none,vAlignment:Spread,noOutputIfInvisible:true</style>
                  <dependsOn>Farm branches:Dairy,Fattners,Beef,MotherCows,Sows</dependsOn>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Silos</Title>
                  <Value>silos_de</Value>
                  <options>%datdir%/silos_*.gms</options>
                  <gamsName>siloFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>output:none,vAlignment:Spread</style>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Buildings</Title>
                  <Value>buildings_de</Value>
                  <options>%datdir%/buildings_*.gms</options>
                  <gamsName>buildingsFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>output:none,vAlignment:Spread</style>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>Cattle</Title>
                  <Value>cattle_de</Value>
                  <options>%datdir%/cattle_*.gms</options>
                  <gamsName>cattleFile</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>output:none,vAlignment:Spread,noOutputIfInvisible:true</style>
                  <dependsOn>Farm branches:Dairy,Beef,MotherCows</dependsOn>
         </control>

         <control>
                  <Type>separator</Type>
                  <Title>Simulation control</Title>
                  <tasks>%allTasks%</tasks>
         </control>

         <control>
                  <Type>checkbox</Type>
                  <Title>Stochastic programming</Title>
                  <Value>false</Value>
                  <gamsName>StochProg</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>vAlignment:Spread</style>
         </control>
         <control>
                  <Type>checkbox</Type>
                  <Title>Post-Model stochastics</Title>
                  <Value>false</Value>
                  <gamsName>PostModelStoch</gamsName>
                  <tasks>%allTasks%</tasks>
                  <style>vAlignment:Spread</style>
         </control>

         <control>
                  <Type>checkbox</Type>
                  <Title>Allow for off farm work</Title>
                  <Value>false</Value>
                  <gamsName>allowForOffFarm</gamsName>
                  <style>vAlignment:Spread</style>
                  <tasks>%allTasks%</tasks>
         </control>

         <control>
                  <Type>checkbox</Type>
                  <Title>Allow hiring full-time farm workers</Title>
                  <Value>false</Value>
                  <gamsName>allowHiring</gamsName>
                  <style>vAlignment:Spread,title:Allow hiring farm workers</style>
                  <tasks>%allTasks%</tasks>
         </control>

         <control>
                  <Type>checkBox</Type>
                  <Title>Allow buying or selling land</Title>
                  <tooltip><![CDATA[If switched on allows for buying and selling land (selling part included in liquidation of <BR/> last year, but only if not run in comparative-static mode).]]></tooltip>
                  <Value>false</Value>
                  <gamsName>landbuy</gamsName>
                  <style>vAlignment:Spread</style>
                  <tasks>%allTasks%</tasks>
         </control>

         <control>
                  <Type>Spinner</Type>
                  <Title>Max land bought per plot [ha]</Title>
                  <Value>10</Value>
                  <range>0,100,1</range>
                  <gamsName>maxLandBuy</gamsName>
                  <style>vAlignment:Spread</style>
                  <dependsOn>Allow buying or selling land:true</dependsOn>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>checkBox</Type>
                  <Title>Allow leasing land</Title>
                  <Value>false</Value>
                  <gamsName>landlease</gamsName>
                  <style>vAlignment:Spread</style>
                  <tasks>%allTasks%</tasks>
         </control>


         <control>
                  <Type>Spinner</Type>
                  <Title>Land rent [Euro/ha]</Title>
                  <Value>310</Value>
                  <options>int</options>
                  <range>0,1000,1</range>
                  <gamsName>landrent</gamsName>
                  <dependsOn>Allow leasing land:true/Allow buying or selling land:true</dependsOn>
                  <style>vAlignment:Spread</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Contract length</Title>
                  <Value>10</Value>
                  <range>0,30,1</range>
                  <options>int</options>
                  <gamsName>rentContractLength</gamsName>
                  <dependsOn>Allow leasing land:true</dependsOn>
                  <style>vAlignment:Spread,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>separator</Type>
                  <Title>Calibration</Title>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>checkBox</Type>
                  <Title>Calibrate</Title>
                  <Value>false</Value>
                  <gamsName>calibration</gamsName>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>fileSel</Type>
                  <Title>File with calibration bounds</Title>
                  <Value>farm_testit</Value>
                  <options>%modeldir%/calibFiles/farm_*.gms</options>
                  <gamsName>calibFile</gamsName>
                  <tasks>Single farm run</tasks>
                  <style>title:File with\ncalibration bounds,output:none,noOutputIfInvisible:true</style>
                  <dependsOn>Calibrate:true</dependsOn>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Yields</Title>
                  <Value>20</Value>
                  <options>int</options>
                  <range>0,50,1</range>
                  <gamsName>yieldCalib</gamsName>
                  <dependsOn>Calibrate:true</dependsOn>
                  <style>spread:true,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Output prices</Title>
                  <Value>20</Value>
                  <options>int</options>
                  <range>0,50,1</range>
                  <gamsName>outputPriceCalib</gamsName>
                  <dependsOn>Calibrate:true</dependsOn>
                  <style>sameline:true,spread:true,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Cost</Title>
                  <Value>20</Value>
                  <options>int</options>
                  <range>0,50,1</range>
                  <gamsName>costCalib</gamsName>
                  <dependsOn>Calibrate:true</dependsOn>
                  <style>spread:true,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Input prices</Title>
                  <Value>20</Value>
                  <options>int</options>
                  <range>0,50,1</range>
                  <gamsName>inputPriceCalib</gamsName>
                  <dependsOn>Calibrate:true</dependsOn>
                  <style>sameline:true,spread:true,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Feed requirements</Title>
                  <Value>20</Value>
                  <options>int</options>
                  <range>0,50,1</range>
                  <gamsName>reqCalib</gamsName>
                  <dependsOn>Calibrate:true</dependsOn>
                  <style>title:Feed\ncoefficients,spread:true,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Labour requirements</Title>
                  <Value>20</Value>
                  <options>int</options>
                  <range>0,50,1</range>
                  <gamsName>labCalib</gamsName>
                  <dependsOn>Calibrate:true</dependsOn>
                  <style>title:Labour\ncoefficients,sameline:true,spread:true,noOutputIfInvisible:true</style>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Lower bounds on ojective [%]</Title>
                  <Value>95</Value>
                  <options>int</options>
                  <range>0,100,1</range>
                  <gamsName>objeLimitLow</gamsName>
                  <style>noOutputIfInvisible:true</style>
                  <dependsOn>Calibrate:true</dependsOn>
                  <tasks>%allTasks%</tasks>
         </control>
         <control>
                  <Type>Spinner</Type>
                  <Title>Upper bounds on ojective [%]</Title>
                  <Value>105</Value>
                  <options>int</options>
                  <range>90,150,1</range>
                  <gamsName>objeLimitUpp</gamsName>
                  <style>noOutputIfInvisible:true</style>
                  <dependsOn>Calibrate:true</dependsOn>
                  <tasks>%allTasks%</tasks>
